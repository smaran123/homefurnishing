
<!-- Content Area -->
<div id="da-content-area">
  <div class="grid_2">
    <div class="da-panel">
      <div class="da-panel-header">
        <span class="da-panel-title">
          <img src="/assets/admin/images/icons/black/16/pencil.png" alt="" />
          New Product
        </span>
      </div>
      <div class="da-panel-content">
        <%= nested_form_for([current_admin, @product], :url => admin_product_path(:id => @product.id), :html => { :multipart => true, :class => "da-form" }, :method => "PUT") do |f| %>
          <%= render :partial => 'form', :locals => {:f => f} %>
        <% end %>
      </div>
    </div>
  </div>
</div>